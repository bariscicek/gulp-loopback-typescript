import { <%- modelBaseName %> } from "../index";

/* tslint:disable */
<%- buildModelImports(model) %>
export interface I<%- modelName %> {
<%- buildModelProperties(model, true) %>
}

export declare class <%- modelName %> extends PersistedModel implements I<%- modelName %> {
<%- buildModelProperties(model, true) %>
  /**
   * The name of the model represented by this $resource,
   * i.e. `<%- modelName %>`.
   */
  public static getModelName() {
    return <%-: modelName | q %>;
  }
  /**
  * @method factory
  * This method creates an instance of <%- modelName %> for dynamic purposes.
  **/
  public static factory(data: I<%- modelName %>): <%- modelName %>{
    return new <%- modelName %>();
  }  
  /**
  * @method getModelDefinition
  * This method returns an object that represents some of the model
  * definitions.
  **/
  public static getModelDefinition() {
    return {
      name: '<%- modelName %>',
      plural: '<%- plural %>',
      properties: {<% for ( var prop in model.properties ) { %>
        <%= prop %>: {
          name: '<%= prop %>',
          type: '<%- buildPropertyType( model.properties[prop].type, prop, model.properties[prop] ) %>'<% if ( model.properties[prop].hasOwnProperty( "default" ) ) { %>,
          default: <%- buildPropertyDefaultValue( model.properties[prop], prop, model.properties[prop] ) %><% } %>
        },<% } %>
      },
      relations: {<% for ( var rel in model.relations ) { %>
        <%= rel %>: {
          name: '<%= rel %>',
          type: '<%- buildRelationType( model, rel ) %>',
          model: '<%- model.relations[rel].targetClass %>'
        },<% } %>
      }
    }
  }
}
